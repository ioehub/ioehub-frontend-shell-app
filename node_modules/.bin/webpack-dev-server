#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/webpack-dev-server@4.15.2_webpack-cli@4.10.0_webpack@5.98.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
