#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules/vite/bin/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules/vite/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules/vite/bin/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules/vite/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.13_terser@5.39.0/node_modules:/mnt/c/vang/project/2025/ioehub/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../vite/bin/vite.js" "$@"
fi
